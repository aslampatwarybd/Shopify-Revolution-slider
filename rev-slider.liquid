      {% for i in (1..5) %}
        {% capture slide %}rev_slide_{{ i }}_enable{% endcapture %}
        {% capture slide_bg_type %}rev_slide_{{ i }}_bg_type{% endcapture %}
        {% capture slide_transition %}rev_slide_{{ i }}_transition{% endcapture %}
        {% capture slide_delay %}rev_slide_{{ i }}_delay{% endcapture %}
        {% capture slide_img %}rev_slide_{{ i }}.jpg{% endcapture %}
        {% capture slide_bg_color %}rev_slide_{{ i }}_bg_color{% endcapture %}
        {% capture slide_video %}rev_slide_{{ i }}_video{% endcapture %}
        {% capture slide_overlay_opacity %}rev_slide_{{ i }}_overlay_opacity{% endcapture %}
        {% capture slide_bgimage_valign %}rev_slide_{{ i }}_bgimage_valign{% endcapture %}
        {% capture slide_styleling %}rev_slide_{{ i }}_slide_styleling{% endcapture %}
        {% capture slide_caption_transition %}rev_slide_{{ i }}_caption_transition{% endcapture %}
        {% capture slide_caption_content_align %}rev_slide_{{ i }}_caption_content_align{% endcapture %}
        {% capture slide_caption_title %}rev_slide_{{ i }}_caption_title{% endcapture %}
        {% capture slide_caption_text %}rev_slide_{{ i }}_caption_text{% endcapture %}
        {% capture slide_button_text %}rev_slide_{{ i }}_button_text{% endcapture %}
        {% capture slide_link %}rev_slide_{{ i }}_link{% endcapture %}
        {% capture slide_link_target %}rev_slide_{{ i }}_link_target{% endcapture %}
        {% capture slide_caption_x %}rev_slide_{{ i }}_caption_x{% endcapture %}
        {% capture slide_caption_y %}rev_slide_{{ i }}_caption_y{% endcapture %}      
        {% capture slide_video_type %}rev_slide_{{ i }}_video_type{% endcapture %}
        {% capture slide_video_loop %}rev_slide_{{ i }}_video_loop{% endcapture %}
        {% capture slide_video_play %}rev_slide_{{ i }}_video_play{% endcapture %}
        {% capture slide_video_volume %}rev_slide_{{ i }}_video_volume{% endcapture %}
        {% capture slide_video_overlay %}rev_slide_{{ i }}_video_overlay{% endcapture %}  
